=\"modal\">\n                    <!-- 컴포넌트 MyModal -->\n                    <MyModal @close=\"closeModal\" v-if=\"modal\">\n                      <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n                        <v-text-field v-model=\"id\" :counter=\"20\"  label=\"id\" required>\n                        </v-text-field>\n\n                        <v-text-field v-model=\"password\" :append-icon=\"show1 ? 'mdi-eye' : 'mdi-eye-off'\"\n                          :rules=\"[rules.required, rules.min]\" :type=\"show1 ? 'text' : 'password'\" name=\"input-10-1\"\n                          label=\"Password\" hint=\"At least 8 characters\" counter @click:append=\"show1 = !show1\">\n                        </v-text-field>\n\n                        <v-checkbox v-model=\"checkbox\" :rules=\"[v => !!v || 'You must agree to continue!']\"\n                          label=\"아이디 저장\" required></v-checkbox>\n\n                        <v-btn :disabled=\"!valid\" color=\"green\" class=\"mr-4\" @click=\"doSend\">\n                          Validate\n                        </v-btn>\n\n                      </v-form>\n                    </MyModal>\n                  </div>\n                </div>\n              </v-card>\n            </v-col>\n          </v-row>\n\n          <v-row no-gutters>\n            <v-col cols=\"12\" md=\"9\">\n              <v-card id=\"jb-contents\">\n                <Map2></Map2>\n              </v-card>\n            </v-col>\n            <v-col cols=\"6\" md=\"3\">\n              <v-card id=\"jb-sidebar\">\n                <Calamity></Calamity>\n              </v-card>\n            </v-col>\n          </v-row>\n          <v-spacer></v-spacer>\n          <div>\n            <h1>&nbsp;</h1>\n          </div>\n          <v-card id=\"jb-footer\">\n            <listTable></listTable>\n          </v-card>\n\n        </v-flex>\n      </v-layout>\n    </v-container>\n  </div>\n</template>\n\n<script>\n  import HelloWorld from './components/HelloWorld';\nimport LoginPro from './components/LoginPro';\nimport Map2 from './components/Map2';\nimport Calamity from './components/Calamity';\nimport listTable from './components/listTable';\nimport Login from './components/Login.vue';\nimport MyModal from './components/LoginForm';\nimport websocket from './components/websocket';\n\n\nexport default {\n  name: 'App',\n\n  components: {\n    HelloWorld,\n    LoginPro,\n    Map2,\n    Calamity,\n    listTable,\n    Login, //modal\n    MyModal,\n    websocket\n  },\n  mounted() {\n    console.log(\"Parent mounted\")\n},\ndata: () => ({\n    //\n    valid: false,\n    checkbox: false,\n    show1: false,\n    password: 'Password',\n    rules: {\n      required: value => !!value || 'Required.',\n      min: v => v.length >= 8 || 'Min 8 characters',\n    },\n    modal: false,\n    id: '',\n    password: '',\n    title: \"제목이오\"\n  }),\n  methods: {\n    openModal() {\n      this.modal = true\n    },\n    closeModal() {\n      this.modal = false\n    },\n    doSend() {\n      if (this.id.length > 0) {\n        alert(this.id+'님 환영합니다.')\n        this.id = ''\n        this.password = ''\n        this.closeModal()\n      } else {\n        alert('메시지를 입력해주세요.')\n      }\n    }\n\n  }\n\n}; \n</script>\n\n\n\n\n<style>#jb-container {\n  width: 1500px;\n  height: 750px;\n  margin: 0px auto;\n  padding: 10px;\n  resize: none;\n  border: 1px solid #bcbcbc;\n}\n\n#jb-header {\n  padding: 20px;\n  margin-bottom: 20px;\n  border: 1px solid #bcbcbc;\n}\n\n#jb-content {\n  width: 1000px;\n  padding: 20px;\n  margin-bottom: 20px;\n  margin-left: 120px;\n  float: left;\n  border: 1px solid #bcbcbc;\n}\n\n#jb-sidebar {\n  width: 300px;\n  height: 670px;\n  padding: 20px;\n  margin-bottom: 20px;\n  float: right;\n  border: 1px solid #bcbcbc;\n}\n\n#jb-footer {\n  clear: both;\n  padding: 0px;\n  margin-top: 20px;\n  border: 1px solid #bcbcbc;\n}\n\n</style>"]}]}