,sBA2EA;AACA,WAAA,GAAA,GAAA,IAAA,IAAA,EAAA;AACA,WAAA,OAAA,GAAA,gBAAA,GAAA,GAAA,KAAA,EAAA,GAAA,GAAA,GAAA,KAAA,QAAA,GAAA,GAAA,GAAA,KAAA,GAAA,CAAA,WAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,OAAA,GAAA,GAAA,GAAA,KAAA,GAAA,CAAA,WAAA,EAAA;AACA,WAAA,WAAA,CAAA,KAAA,OAAA;AACA,KAhFA;AAiFA,IAAA,iBAjFA,6BAiFA,MAjFA,EAiFA;AACA,aAAA,IAAA,OAAA,CAAA,UAAA,OAAA,EAAA,OAAA,EAAA;AACA,eAAA,MAAA,CAAA,UAAA,KAAA,MAAA,CAAA,IAAA,EAAA;AAAA;AAAA;;AACA,eAAA,OAAA,EAAA;AACA,OAHA,CAAA;AAIA,KAtFA;AAuFA,IAAA,IAvFA,kBAuFA;AACA,WAAA,GAAA,IAAA,MAAA;AACA,WAAA,GAAA,IAAA,MAAA;AACA,KA1FA;AA2FA,IAAA,QA3FA,sBA2FA;AACA,UAAA,QAAA,GAAA,IAAA,CAAA,MAAA,EAAA;AACA,WAAA,GAAA,GAAA,IAAA,CAAA,KAAA,CAAA,QAAA,GAAA,MAAA,GAAA,MAAA,CAAA;AACA,WAAA,OAAA,CAAA,CAAA,IAAA,KAAA,GAAA;AACA,UAAA,QAAA,GAAA,IAAA,CAAA,MAAA,EAAA;AACA,WAAA,GAAA,GAAA,IAAA,CAAA,KAAA,CAAA,QAAA,GAAA,MAAA,GAAA,OAAA,CAAA;AACA,WAAA,OAAA,CAAA,CAAA,IAAA,KAAA,GAAA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,MAAA,EAAA;AACA,WAAA,MAAA,GAAA,IAAA,CAAA,KAAA,CAAA,KAAA,GAAA,IAAA,GAAA,GAAA,CAAA;AACA,WAAA,OAAA,CAAA,CAAA,IAAA,KAAA,MAAA;AAEA,MAAA,QAAA,CAAA,KAAA,CAAA,cAAA,EAAA,KAAA,OAAA;AAEA;AAxGA;AAlDA,CAAA","sourcesContent":["<template>\n  <div id=\"app\" style=\"width:1900px;\" align=\"center\">\n    <v-container class=\"grey lighten-5\" fluid>\n      <v-layout>\n        <v-flex sm=\"9\" id=\"jb-container\">\n          <div id=\"titlebar\">\n          <v-row no-gutters>\n            <v-col cols=\"12\" sm=\"6\" md=\"9\">\n              <div class=\"pa-2\" outlined tile>\n                <h1 align=\"left\" style=\"color:white;\">Segment 긴급 메시지 전송 시스템</h1> \n              </div>\n            </v-col>\n            <v-col cols=\"6\" md=\"3\">\n                 <!-- 로그인 페이지 -->\n                 <div class=\"example-modal-window\">\n                  <div v-if=\"!loginCondition\">\n                    <button @click=\"openModal\" style=\"color:white;\"><h1>로그인</h1></button>\n                    <div v-if=\"modal\">\n                      <!-- 컴포넌트 MyModal -->\n                      <MyModal @close=\"closeModal\" v-if=\"modal\">\n                        <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n                          <v-text-field v-model=\"id\" :counter=\"20\" label=\"id\" required>\n                          </v-text-field>\n\n                          <v-text-field v-model=\"password\" :append-icon=\"show1 ? 'mdi-eye' : 'mdi-eye-off'\"\n                            :rules=\"[rules.required, rules.min]\" :type=\"show1 ? 'text' : 'password'\" name=\"input-10-1\"\n                            label=\"Password\" hint=\"At least 8 characters\" counter @click:append=\"show1 = !show1\">\n                          </v-text-field>\n\n                          <v-checkbox v-model=\"checkbox\" :rules=\"[v => !!v || 'You must agree to continue!']\"\n                            label=\"아이디 저장\" required></v-checkbox>\n\n                          <v-btn :disabled=\"!valid\" color=\"green\" class=\"mr-4\" @click=\"tryLogin\">\n                            Validate\n                          </v-btn>\n                        </v-form>\n                      </MyModal>\n                    </div>\n                  </div>\n\n                  <div v-if=\"loginCondition\">\n                    <p style=\"font-weight:bold\">{{id}}</p>님 환영합니다.\n                  </div>\n                </div>\n                  <v-btn class=\"mr-4\" @click=\"drawTest\">propsTest</v-btn>\n            </v-col>\n          </v-row>\n        </div>\n        <div>\n          <v-row no-gutters>\n            <v-col cols=\"12\" md=\"10\">\n              <div id=\"jb-contents\">\n                <Map2 :pointX=\"lat\" :pointY=\"log\" :radius=\"radius\"></Map2>\n              </div>\n            </v-col>\n            <v-col cols=\"6\" md=\"2\">\n              <div id=\"jb-sidebar\" fluid>\n                <Calamity :pointX=\"lat\" :pointY=\"log\"></Calamity>\n              </div>\n            </v-col>\n          </v-row>\n        </div>\n          <v-spacer></v-spacer>\n          <div>\n            <h1>&nbsp;</h1>\n          </div>\n          <v-card id=\"jb-footer\">\n            <listTable></listTable>\n          </v-card>\n\n        </v-flex>\n      </v-layout>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport HelloWorld from './components/HelloWorld';\nimport LoginPro from './components/LoginPro';\nimport Map2 from './components/Map2';\nimport Calamity from './components/Calamity';\nimport listTable from './components/listTable';\nimport Login from './components/Login.vue';\nimport MyModal from './components/LoginForm';\nimport {EventBus} from \"./components/event-bus\";\n\nexport default {\n  name: 'App',\n\n  components: {\n    HelloWorld,\n    LoginPro,\n    Map2,\n    Calamity,\n    listTable,\n    Login, //modal\n    MyModal\n  },\n  mounted() {\n    console.log(\"Parent mounted\")\n    this.connect();\n    //this.openModal();\n  },\n  data(){\n\t\treturn{\n\t\t\t valid: false,\n    checkbox: false,\n    show1: false,\n    password: 'Password',\n    rules: {\n      required: value => !!value || 'Required.',\n      min: v => v.length >= 8 || 'Min 8 characters',\n    },\n    modal: false,\n    id: '',\n    password: '',\n    title: \"제목이오\",\n    message: \"\",\n    logs: [],\n    status: \"disconnected\",\n    now :'',\n    lat : 990000.60652286,\n    log : 1720034.9139856,\n    loginCondition : false,\n    radius : 10,\n    \n    caldata :[{\n      lat2 : 0,\n      log2 : 0,\n      radius2 : 0\n    }]\n\n    }\n\t},\n    //\n   \n  methods: {\n    openModal() {\n      this.modal = true\n    },\n    closeModal() {\n      this.modal = false\n    },\n    doSend() {\n      if (this.id.length > 0) {\n        alert(this.id + '님 환영합니다.')\n        this.id = ''\n        this.password = ''\n        this.closeModal()\n      } else {\n        alert('메시지를 입력해주세요.')\n      }\n    },\n\n    connect() {\n      //this.socket = new WebSocket(\"ws://158.181.17.81:8080/smart/websocket\");\n      this.socket = new WebSocket(\"wss://echo.websocket.org\");\n      \n      //wss:echo.websocket.org\n      console.log('소켓 연결'+this.socket)\n\n      this.socket.onopen = () => {\n        this.status = \"connected\";\n        this.logs.push({\n          event: \"연결 완료: \",\n          //data: 'ws://158.181.17.81:8080/smart/websocket'\n          data: 'wss://echo.websocket.org'\n        })//ip변경시 문제점 해결\n        this.socket.onmessage = ({\n          data\n        }) => {\n          this.logs.push({\n            event: \"메세지 수신\",\n            data\n          });\n\n          //관리자가 들어왔을때 라고 가정 \n          if(data.indexOf('admin')!==-1){\n            this.closeModal()\n            alert('admin님 환영합니다.')\n            this.loginCondition = true;\n          }\n          else if(data.indexOf('calamity')!==-1){\n            this.closeModal()\n\n          }\n           else if(data.indexOf('drawCircle')!==-1){\n            this.drawTest()\n\n          }\n          console.log(data+'수신')\n        };\n      };\n      console.log(this.status)\n    },\n    disconnect() {\n      this.socket.close();\n      console.log('연결 종료')\n      this.status = \"disconnected\";\n      this.logs = [];\n    },\n    sendMessage(e) {\n      //waitForOpenSocket(socket)\n      this.socket.send(this.message);\n      console.log(this.message+'전송')\n      this.logs.push({\n        event: \"메시지 전송\",\n        data: this.message\n      });\n      this.message = \"\";\n    },\n  tryLogin(){\n    this.now = new Date();\n    this.message = '[Logindtry]'+'$'+this.id +'$'+ this.password +'$'+ this.now.toUTCString; \n    console.log(this.message+' '+this.now.toUTCString())\n    this.sendMessage(this.message);\n  },\n waitForOpenSocket(socket) {\n  return new Promise((resolve, _reject) => {\n    while (socket.readyState !== socket.OPEN) { /* no-op */ }\n    return resolve()\n  })\n},\ntest(){\n  this.lat +=1000.1\n  this.log +=1000.1\n},\ndrawTest(){\n  var jbRandom = Math.random();\n  this.lat = Math.floor( jbRandom*100000 + 900000)\n  this.caldata[0] = this.lat;\n  var jcRandom = Math.random();\n  this.log = Math.floor( jcRandom*100000 + 1600000)\n  this.caldata[1] = this.log;\n  var ranra = Math.random();\n  this.radius = Math.floor( ranra*1000 + 100,)\n  this.caldata[2] = this.radius;\n\n  EventBus.$emit(\"use-eventbus\",this.caldata);\n  \n}\n\n\n  }\n}; </script>\n\n\n\n\n<style>\n#titlebar{\nbackground-color:#3f5261;\nmargin-bottom : 20px;\n}\n\n#jb-container {\n  width: 1500px;\n  height: 750px;\n  margin: 0px auto;\n  padding: 10px;\n  resize: none;\n  border: 0px solid #bcbcbc;\n}\n\n#jb-header {\n  padding: 20px;\n  margin-bottom: 0px;\n  border: 0px solid #bcbcbc;\n}\n\n#jb-content {\n  width: 1000px;\n  padding: 20px;\n  margin-bottom: 0px;\n  margin-left: 120px;\n  float: left;\n  border: 0px solid #bcbcbc;\n}\n\n#jb-sidebar {\n  width: 300px;\n  height: 670px;\n  padding: 20px;\n  margin-bottom: 0px;\n  float: right;\n  border: 0px solid #bcbcbc;\n}\n\n#jb-footer {\n  clear: both;\n  padding: 0px;\n  margin-top: 0px;\n  border: 0px solid #bcbcbc;\n}\n\n</style>"],"sourceRoot":"src"}]}