2C;AACnC;;AADmC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAhBM,cAAAA,GAAgB,SAAhBA,GAAgB,EAAXC,QAAW,SAAXA,QAAW;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEpC;AANP,2BAOeP,KAPf,SAOuC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAhBM,YAAAA,GAAgB,SAAhBA,GAAgB,EAAXC,QAAW,SAAXA,QAAW;AAAA;AAAA,mBACTd,GAAG,CAACY,KAAJ,CAAUC,GAAV,EAAeC,QAAf,CADS;;AAAA;AAC/BN,YAAAA,aAD+B;AAEnCF,YAAAA,eAAe,CAACC,KAAD,EAAQC,aAAR,CAAf;AAFmC,8CAG5BD,KAAK,CAACQ,OAAN,CAAcC,SAHc;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIpC,CAXH","sourcesContent":["import {UID, IS_AUTH, ERROR_STATE} from './mutation_types'\r\nimport api from '../service'\r\n\r\nlet setUID = ({commit}, data) => {\r\n  commit(UID, data)\r\n}\r\n\r\nlet setErrorState = ({commit}, data) => {\r\n  commit(ERROR_STATE, data)\r\n}\r\n\r\nlet setIsAuth = ({commit}, data) => {\r\n  commit(IS_AUTH, data)\r\n}\r\n\r\n// 백엔드에서 반환한 결과값을 가지고 로그인 성공 실패 여부를 vuex에 넣어준다.\r\nlet processResponse = (store, loginResponse) => {\r\n  switch (loginResponse) {\r\n    case 'noAuth':\r\n      setErrorState(store, 'Wrong ID or Password')\r\n      setIsAuth(store, false)\r\n      break\r\n    case 'done':\r\n      setErrorState(store, 'No period')\r\n      setIsAuth(store, false)\r\n      break\r\n    default:\r\n      setUID(store, loginResponse.UID)\r\n      setErrorState(store, '')\r\n      setIsAuth(store, true)\r\n  }\r\n}\r\n\r\nexport default {\r\n    changeStateAction(context,payload) {     //context로 store의 메서드와 속성 접근\r\n        context.commit('changeState',payload);\r\n      },\r\n      async login (store, {uid, password}) {\r\n        /* 로그인은 백엔드를 다녀와야 하냐 비동기 처리를 한다 */\r\n      },\r\n  async login (store, {uid, password}) {\r\n    let loginResponse = await api.login(uid, password)\r\n    processResponse(store, loginResponse)\r\n    return store.getters.getIsAuth  // 로그인 결과를 리턴한다\r\n  }\r\n}"]}]}