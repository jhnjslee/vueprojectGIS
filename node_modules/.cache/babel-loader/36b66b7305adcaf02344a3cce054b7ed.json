his.modal = true\n    },\n    closeModal() {\n      this.modal = false\n    },\n    doSend() {\n      if (this.id.length > 0) {\n        alert(this.id+'님 환영합니다.')\n        this.id = ''\n        this.password = ''\n        this.closeModal()\n      } else {\n        alert('메시지를 입력해주세요.')\n      }\n    }\n\n  }\n\n}; \n</script>\n\n\n\n\n<style>#jb-container {\n  width: 1500px;\n  height: 750px;\n  margin: 0px auto;\n  padding: 10px;\n  resize: none;\n  border: 1px solid #bcbcbc;\n}\n\n#jb-header {\n  padding: 20px;\n  margin-bottom: 20px;\n  border: 1px solid #bcbcbc;\n}\n\n#jb-content {\n  width: 1000px;\n  padding: 20px;\n  margin-bottom: 20px;\n  margin-left: 120px;\n  float: left;\n  border: 1px solid #bcbcbc;\n}\n\n#jb-sidebar {\n  width: 300px;\n  height: 670px;\n  padding: 20px;\n  margin-bottom: 20px;\n  float: right;\n  border: 1px solid #bcbcbc;\n}\n\n#jb-footer {\n  clear: both;\n  padding: 0px;\n  margin-top: 20px;\n  border: 1px solid #bcbcbc;\n}\n\n</style>"],"sourceRoot":"src"}]}